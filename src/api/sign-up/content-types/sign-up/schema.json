{
  "kind": "collectionType",
  "collectionName": "sign_ups",
  "info": {
    "singularName": "sign-up",
    "pluralName": "sign-ups",
    "displayName": "registration",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "fullName": {
      "type": "string",
      "required": true,
      "unique": false,
      "minLength": 2,
      "maxLength": 70
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "password": {
      "type": "password",
      "required": true
    },
    "socialSecurityNumber": {
      "type": "biginteger",
      "required": true,
      "unique": true,
      "min": "9"
    },
    "projectSelection": {
      "type": "enumeration",
      "enum": [
        "Project Name 1",
        "Project Name 2"
      ]
    },
    "contractorLicense": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "approver": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "project": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::project.project",
      "inversedBy": "registrations"
    }
  }
}
