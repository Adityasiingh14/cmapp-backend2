{
  "kind": "collectionType",
  "collectionName": "registrations",
  "info": {
    "singularName": "registration",
    "pluralName": "registrations",
    "displayName": "registration"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "fullName": {
      "type": "string"
    },
    "socialSecurityNumber": {
      "type": "biginteger"
    },
    "email": {
      "type": "email"
    },
    "password": {
      "type": "password"
    },
    "project": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::project.project",
      "inversedBy": "registrations"
    },
    "documents": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "approver": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
