{
  "kind": "collectionType",
  "collectionName": "registrations",
  "info": {
    "singularName": "registration",
    "pluralName": "registrations",
    "displayName": "Registration",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "fullname": {
      "type": "string"
    },
    "socialSecurityNumber": {
      "type": "biginteger"
    },
    "email": {
      "type": "email"
    },
    "documents": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "password": {
      "type": "string"
    },
    "project": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::project.project",
      "inversedBy": "registrations"
    },
    "approver": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "label": {
      "type": "enumeration",
      "enum": [
        "approved",
        "pending",
        "rejected"
      ]
    }
  }
}
